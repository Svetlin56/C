int main()
{
    char name[25];
    int age;
    printf("Whats your name\n");
    scanf("%s", &name);

    printf("How old are you\n");
    scanf("%d", &age);

    printf("\nHello %s, how are you?", name);
    printf("\nYou are %d years old.", age);

    return 0;
}



int main()
{
    double A = sqrt(9);
    int C = round(3.14);
    int E = floor(3.99);
    double F = fabs(-100);
    printf("\n%d",C);
    return 0;
}
